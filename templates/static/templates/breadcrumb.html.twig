<div class="breadcrumb_nav_wrapper">
	<div class="breadcrumb_nav">
		<div class="breadcrumb_list" itemscope itemtype="http://schema.org/BreadcrumbList">
			{% for link in breadcrumb %}
				<div class="breadcrumb_item" itemscope itemprop="itemListElement" itemtype="http://schema.org/ListItem">
					{% if loop.last %}
						<span class="breadcrumb_label" itemprop="item">
							<span class="breadcrumb_name" itemprop="name">{{ link.text }}</span>
						</span>
					{% else %}
						<a class="breadcrumb_link" href="{{ link.url }}" itemprop="item">
							{% if loop.first %}
							<svg aria-hidden="true" class="symbol symbol_home symbol_dark">
								<use xlink:href="#home"></use>
							</svg>
							<span class="breadcrumb_name" itemprop="name">Home</span>
							{% else %}
							<span class="breadcrumb_name" itemprop="name">{{ link.text }}</span>
							{% endif %}
						</a>
					{% endif %}
					<meta itemprop="position" content="{{ loop.index + 1 }}">
				</div>
			{% endfor %}
		</div>
	</div>
</div>