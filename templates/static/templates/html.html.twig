{%
	set body_classes = [
		'fs-grid',
		logged_in ? 'user-logged-in',
		not root_path ? 'path-frontpage' : 'path-' ~ root_path|clean_class,
		node_type ? 'page-node-type-' ~ node_type|clean_class,
		db_offline ? 'db-offline',
		is_front_page ? 'theme_home' : 'theme_default',
		is_wayfinding ? 'theme_wayfinding' : ''
	]
%}
<!DOCTYPE html>
<html class="no-js" lang="en" itemscope="" itemtype="http://schema.org/WebPage"{{ html_attributes }}>
	<head>
		<head-placeholder token="{{ placeholder_token }}">

		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<meta name="apple-mobile-web-app-capable" content="yes">
		<meta name="mobile-web-app-capable" content="yes">
		<meta name="description" content="">

		<title>{{ head_title|safe_join(' | ') }}</title>

		<script src="https://use.typekit.net/yvv5nyp.js"></script>
		<script>try{Typekit.load({ async: true });}catch(e){}</script>
		<script src="{{ static_root }}js/modernizr.js"></script>

		<css-placeholder token="{{ placeholder_token }}">
		<js-placeholder token="{{ placeholder_token }}">
	</head>

	<body{{ attributes.addClass(body_classes) }}>
		<figure style="display: none;">
			{% include directory ~ '/images/svg/sprite.symbol.svg' %}
		</figure>

		<!-- Skip-to-Content Link -->
		<a class="skip_link" id="skip_to_content" href="#main_content" >Skip to Main Content</a>

		{{ page_top }}
		{{ page }}
		{{ page_bottom }}
		<script type="text/javascript">
		var WWW_ROOT = "{{ static_root}}";
		</script>

		<script>
		  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
		  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
		  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
		  })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
		  ga('create', 'UA-3585362-1', 'auto');
		  ga('send', 'pageview');
		</script>

		<js-bottom-placeholder token="{{ placeholder_token }}">

		{# <img src="http://leadback.advertising.com/adcedge/lb?site=695501&amp;srvc=1&amp;betr=57228=1671531[4320]" width="1" height="1" border="0" style="display: none !important;"> #}
	</body>
</html>
