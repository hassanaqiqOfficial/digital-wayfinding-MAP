<script>
	var FlightURL = "{{ url('<front>') }}themes/custom/bwi/cache/flight-data.json";
</script>
<!-- Page Wrapper -->
<div class="js-navigation_push page_wrapper">
	<!-- Header -->
	<header class="header" id="header" itemscope itemtype="http://schema.org/WPHeader">
		<div class="header_ribbon">
			{% include directory ~ "/templates/partials/logo.twig" with {
				modifier: "header"
			} %}

			{{ page.main_navigation }}

			<div class="header_aside">
				{{ page.persistent_navigation }}

				<a class="mobile_sidebar_handle" href="#mobile_sidebar">
					<span class="js-nav_handle mobile_sidebar_handle_label">Menu</span>
					<span class="mobile_sidebar_handle_icon"></span>
				</a>

				{% include directory ~ "/templates/partials/search.twig" with {
					modifier: "lg",
					placeholder: "Search...",
					buttonText: "submit"
				} %}

				<a class="site_search_handle" href="#site_search_lg">
					<span class="site_search_handle_label">Search</span>
					<span class="site_search_handle_scope">
						<svg aria-hidden="true" class="symbol symbol_search symbol_red">
							<use xlink:href="#search"></use>
						</svg>
					</span>
					<span class="site_search_handle_close">
						<svg aria-hidden="true" class="symbol symbol_close symbol_black">
							<use xlink:href="#close"></use>
						</svg>
					</span>
				</a>
			</div>
		</div>
	</header>
	<!-- END: Header -->

	<!-- Page -->
	<div class="page">
		<section class="site_message" aria-hidden="true">
			<div class="row">
				<div class="site_message_description">
					<p>It looks like you're trying to zoom in on this page. For best results: use the most recent version of your browser, disable your browser's 'zoom text only' setting, and use your browser's default font size settings.</p>
					<p>To zoom in, use [Ctrl] + [+] in Windows, and [Cmd] + [+] on a Mac. To zoom out, use the keyboard shortcut [Ctrl] + [-] in Windows and [Cmd] + [-] on a Mac.</p>
				</div>
			</div>
		</section>
		{% if not layout %}
		<div class="subnav_cell subnav_cell_sm">
			{{ page.subnav }}
		</div>
		{% endif %}

		<!-- Page Feature -->
		<div class="page_feature">
			{% if alert %}
				{% include directory ~ "/templates/partials/alert.twig" with { alert: alert } %}
			{% endif %}

			{{ page.feature }}
		</div>
		<!-- END: Page Feature -->

		<!-- Page Content -->
		<div class="page_content">
			<main class="main_content" id="main_content" itemprop="mainContentOfPage">
				<div class="full_width_callouts">
					<div class="flight_search_block">
						<div class="row">
							{{ page.breadcrumb }}

							<div class="flight_search_details">
								<h1 class="flight_search_details_title">{{ title }}</h1>

								<div class="flight_search_details_body">
									<div class="flight_search_caption">{{ page.content }}</div>
										<nav class="flight_search_explore">
											<span class="flight_search_explore_label">Explore This Section:</span>

											<ul class="flight_search_explore_list">
												<li class="flight_search_explore_item">
													<a class="flight_search_explore_link" href="{{ path('entity.node.canonical', {'node': 34 }) }}">
														<span>Airlines</span>
														<svg class="symbol symbol_arrow_right symbol_gray">
															<use xlink:href="#arrow_right"></use>
														</svg>
													</a>
												</li>
												<li class="flight_search_explore_item">
													<a class="flight_search_explore_link" href="{{ path('entity.node.canonical', {'node': 143 }) }}">
														<span>Air Traffic Map</span>
														<svg class="symbol symbol_arrow_right symbol_gray">
															<use xlink:href="#arrow_right"></use>
														</svg>
													</a>
												</li>
												<li class="flight_search_explore_item">
													<a class="flight_search_explore_link" href="{{ path('entity.node.canonical', {'node': 142 }) }}">
														<span>BWI Route Map</span>
														<svg class="symbol symbol_arrow_right symbol_gray">
															<use xlink:href="#arrow_right"></use>
														</svg>
													</a>
												</li>
											</ul>
										</nav>
									</div>
								</div>
							</div>
						</div>

						<section class="schedule" id="schedule">
							<div class="row">
								<div class="schedule_body">
									<div class="schedule_switches">
										<button class="js-swap schedule_switch" data-swap-group="schedule_switch" data-swap-target=".schedule_section_1" data-swap-options='{"collapse": false}'{% if arrival_departure == "arrival" or not arrival_departure %} data-swap-active="true"{% endif %}>Arrivals</button>
										<button class="js-swap schedule_switch" data-swap-group="schedule_switch" data-swap-target=".schedule_section_2" data-swap-options='{"collapse": false}'{% if arrival_departure == "departure" %} data-swap-active="true"{% endif %}>Departures</button>
									</div>
									<div class="fs-dropdown_wrapper schedule_dropdown_wrapper">
										<select class="js-dropdown schedule_dropdown">
											<option value="1">Arrivals</option>
											<option value="2">Departures</option>
										</select>
										<span class="fs-dropdown_icon schedule_dropdown_icon">
											<svg class="symbol symbol_caret_down symbol_red">
												<use xlink:href="#caret_down"></use>
											</svg>
										</span>
									</div>
									<div class="schedule_airline_switcher type_airline">
								<select class="schedule_airline_switcher_select"></select>
							</div>
							<div class="schedule_section schedule_section_1">
								<div class="schedule_list">
									<table class="schedule_table schedule_table_lg">
										<tr class="schedule_header_row">
											<th class="schedule_header contains_action header_condensed type_sched.">
												<span class="schedule_header_label">Sched.</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
											<th class="schedule_header contains_action header_condensed type_update">
												<span class="schedule_header_label">Update</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
											<th class="schedule_header contains_action header_condensed type_to">
												<span class="schedule_header_label">From</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
											<th class="schedule_header contains_action type_airline">
												<select class="schedule_header_select">
												</select>
											</th>
											<th class="schedule_header contains_action header_condensed type_flight #">
												<span class="schedule_header_label">Flight #</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
											<th class="schedule_header contains_action type_gate">
												<span class="schedule_header_label">Gate</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
											<th class="schedule_header contains_action type_baggage claim">
												<span class="schedule_header_label">Baggage Claim</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
											<th class="schedule_header contains_action type_status">
												<span class="schedule_header_label">Status</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
										</tr>
									</table>
									<table class="schedule_table schedule_table_sm">
										<tr class="schedule_header_row">
											<th class="schedule_header contains_action header_condensed type_sched.">
												<span class="schedule_header_label">Sched.</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
											<th class="schedule_header contains_action header_condensed type_update">
												<span class="schedule_header_label">Update</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
											<th class="schedule_header contains_action header_condensed type_to">
												<span class="schedule_header_label">From</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
											<th class="schedule_header contains_action header_condensed type_flight #">
												<span class="schedule_header_label">Flight #</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
										</tr>
									</table>
									<div class="schedule_no_results not-visible">
										<p>No relevant flights found: Try broadening your search above.</p>
									</div>
								</div>
							</div>
							<div class="schedule_section schedule_section_2">
								<div class="schedule_list">
									<table class="schedule_table schedule_table_lg">
										<tr class="schedule_header_row">
											<th class="schedule_header contains_action header_condensed type_sched.">
												<span class="schedule_header_label">Sched.</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
											<th class="schedule_header contains_action header_condensed type_update">
												<span class="schedule_header_label">Update</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
											<th class="schedule_header contains_action header_condensed type_to">
												<span class="schedule_header_label">To</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
											<th class="schedule_header contains_action type_airline">
												<select class="schedule_header_select">
												</select>
											</th>
											<th class="schedule_header contains_action header_condensed type_flight #">
												<span class="schedule_header_label">Flight #</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
											<th class="schedule_header contains_action type_gate">
												<span class="schedule_header_label">Gate</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
											<th class="schedule_header contains_action type_baggage claim">
												<span class="schedule_header_label">Baggage Claim</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
											<th class="schedule_header contains_action type_status">
												<span class="schedule_header_label">Status</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
										</tr>
									</table>
									<table class="schedule_table schedule_table_sm">
										<tr class="schedule_header_row">
											<th class="schedule_header contains_action header_condensed type_sched.">
												<span class="schedule_header_label">Sched.</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
											<th class="schedule_header contains_action header_condensed type_update">
												<span class="schedule_header_label">Update</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
											<th class="schedule_header contains_action header_condensed type_to">
												<span class="schedule_header_label">To</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
											<th class="schedule_header contains_action header_condensed type_flight #">
												<span class="schedule_header_label">Flight #</span>
												<svg class="symbol symbol_carets symbol_gray">
													<use xlink:href="#carets"></use>
												</svg>

											</th>
										</tr>
									</table>
									<div class="schedule_no_results not-visible">
										<p>No relevant flights found: Try broadening your search above.</p>
									</div>
								</div>
							</div>
							<div class="schedule_details"></div>
						</div>
					</div>
				</section>
			</div>
		</div>
	</div>

	<footer class="footer" id="footer" itemscope itemtype="http://schema.org/WPFooter">
		<div class="row">
			<div class="footer_column">
				<div class="footer_row">
					<div class="footer_address" itemscope itemtype="http://schema.org/PostalAddress">
						<div class="footer_address_details">
							<h2 class="footer_address_label">Location</h2>
							<span class="footer_address_name" itemprop="name">BWI</span>
							<span class="footer_address_street" itemprop="streetAddress">7050 Friendship Rd, </span>
							<span class="footer_address_city" itemprop="addressLocality">Baltimore, </span>
							<span class="footer_address_state" itemprop="addressRegion">MD </span>
							<span class="footer_address_zip" itemprop="postalCode">21240</span>
						</div>
						<a class="footer_address_link" href="{{ directions_link }}" target="_blank">
							<span>Get Directions</span>
							<svg aria-hidden="true" class="symbol symbol_arrow_right symbol_red">
								<use xlink:href="#arrow_right"></use>
							</svg>
						</a>
					</div>
				</div>
				<div class="footer_pair">
					<div class="footer_row">
						{{ page.helpful_links }}
					</div>
					<div class="footer_row">
						{{ page.social_links }}
					</div>
				</div>
				<div id="google_translate_element"></div>
				<script type="text/javascript">
				function googleTranslateElementInit() {
					new google.translate.TranslateElement({pageLanguage: 'en', layout: google.translate.TranslateElement.InlineLayout.SIMPLE}, 'google_translate_element');
				}
				</script>
				<script type="text/javascript" src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit"></script>
			</div>
			<div class="footer_aside">
				{% include directory ~ "/templates/partials/logo.twig" with {
					modifier: "footer"
				} %}

				<div class="directory">
					<div class="directory_item">
						<img src="{{ governor_image }}" class="directory_item_image" alt="">
						<div class="directory_item_details">
							<h5 class="directory_item_name">{{ governor_name }}</h5>
							<h6 class="directory_item_title">Governor</h6>
						</div>
					</div>

					<div class="directory_item">
						<img src="{{ lt_governor_image }}" class="directory_item_image" alt="">
						<div class="directory_item_details">
							<h5 class="directory_item_name">{{ lt_governor_name }}</h5>
							<h6 class="directory_item_title">Lt. Governor</h6>
						</div>
					</div>
				</div>
			</div>
		</div>
	</footer>
</div>
<!-- END: page_wrapper -->

<div class="js-navigation mobile_sidebar" id="mobile_sidebar" hidden data-navigation-handle=".js-nav_handle" data-navigation-content=".js-navigation_push" data-navigation-options='{"type": "overlay", "gravity": "right"}'>
	<div class="mobile_sidebar_group">
		{% include directory ~ "/templates/partials/search.twig" with {
			modifier: "sm",
			placeholder: "Search...",
			buttonText: "submit"
		} %}

		{% include directory ~ "/templates/partials/hud-nav.twig" %}

		{{ page.mobile_sidebar }}
	</div>

	{{ page.mobile_sidebar_bottom }}
</div>
